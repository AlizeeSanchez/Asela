<!-----------------------------------------------FA GARD------------------------------------------->

<div class="tab-pane fade show active table table-responsive tableFixHead" id="home" role="tabpanel" aria-labelledby="home-tab">
  <table class="table table-hover" >
      <thead>
        <tr>
          <th>Prénom</th>
          <th>Nom</th>
          <th>Téléphone</th>
          <th>Ville</th>
          <th>Adresse</th>
          <th>Animaux possédés</th>
          <th>Type accepté(s)</th>
          <th>En FA</th>
        </tr>
      </thead>
      <tbody class="m-phone">
      <% for(let hostFamilyTab of allHostFamilly.gard){ %>
        <tr data-toggle="modal" data-target=".bd-familyHost-modal-lg<%= hostFamilyTab.id %>">
          <td data-label="Prénom"><%= hostFamilyTab.firstname %></td>
          <td data-label="Nom"><%= hostFamilyTab.lastname %></td>
          <td data-label="Téléphone"><%= hostFamilyTab.number_phone %></td>
          <td data-label="Ville"><%= hostFamilyTab.postal_code %> <%= hostFamilyTab.city %></td>
          <td data-label="Adresse"><%= hostFamilyTab.adress %></td>
          <td data-label="Animaux"><%= hostFamilyTab.pet_composition %></td>
          <td data-label="Animaux accéptés"><%if(hostFamilyTab.kitten_accepted){%>Chaton -<%}%> <%if(hostFamilyTab.cat_accepted){%>Chat -<%}%> <%if(hostFamilyTab.dog_big_accepted){%>Gros chien -<%}%> <%if(hostFamilyTab.dog_middle_accepted){%>Moyen chien -<%}%> <%if(hostFamilyTab.dog_little_accepted){%>Petit chien -<%}%> <%if(hostFamilyTab.puppy_accepted){%>Chiots<%}%></td>
          <td data-label="Animaux Asela"><%= hostFamilyTab.pet_asela %></td>
        </tr>

        <div class="modal fade bd-familyHost-modal-lg<%= hostFamilyTab.id %>" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
          <div class="modal-dialog modal-lg">
            <div class="modal-content" data-article-id="<%= hostFamilyTab.id %>">
              <div class="modal-header">
                <h5 class="modal-title">Questionnaire de la FA N° <%= hostFamilyTab.id %> reçu le <%= hostFamilyTab.date_sending.toLocaleString('en-GB', { timeZone: 'UTC' }) %></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
              </button>
              </div>
              <div class="modal-body">
                <% if(hostFamilyTab.black_list === true){ %> 
                <div class="alert alert-danger" role="alert">
                  Cette famille d'accueil est inscrite en liste rouge !
                </div>
                <%} else if(hostFamilyTab.new === true) {%>
                  <div class="alert alert-info" role="alert">
                    C'est une nouvelle famille d'accueil, elle n'a jamais été contacté.
                  </div>
                <%} else if(hostFamilyTab.nolongercontact === true) {%>
                  <div class="alert alert-warning" role="alert">
                    C'est une ancienne famille d'accueil : <br> <%= hostFamilyTab.comment %>
                  </div>
                <%} else if(hostFamilyTab.disponibility === false) {%>
                  <div class="alert alert-secondary" role="alert">
                    Cette famille d'accueil est indisponible jusqu'au "<%= hostFamilyTab.people_date_accueil %>" ou jusqu'à qu'elle nous recontacte.
                  </div>
                <% } %>
                <form>
                  <div class="form-row">
                    <div class="form-group col-md-4">
                      <label for="inputlastname" > Nom</label>
                      <input type="text" class="form-control" id="inputLastname<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.lastname %>" disabled <% if (hostFamilyTab.black_list === true){ %> style="background-color: #ff474775;" <% } %>>
                    </div>
                    <div class="form-group col-md-4">
                      <label for="inputfirstname">Prénom</label>
                      <input type="text" class="form-control" id="inputFirstname<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.firstname %>" disabled <% if (hostFamilyTab.black_list === true){ %> style="background-color: #ff474775" <% } %>>
                    </div>
                    <div class="form-group col-md-4">
                      <label for="inputBirthday">Date de naissance</label>
                      <input type="text" class="form-control" id="inputBirthday<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.birthday %>" disabled>
                    </div>
                  </div>
                  <div class="form-row">
                  <div class="form-group col-md-6">
                    <label for="inputAddress">Addresse</label>
                    <input type="text" class="form-control" id="inputAddress<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.adress%>">
                  </div>
                    <div class="form-group col-md-4">
                      <label for="inputCity">Ville</label>
                      <input type="text" class="form-control" id="inputCity<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.city %>">
                    </div>
                    <div class="form-group col-md-2">
                      <label for="inputPC">Code Postale</label>
                      <input type="text" class="form-control" id="inputPC<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.postal_code %>">
                    </div>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-2">
                      <label for="inputNP">Téléphone</label>
                      <input type="text" class="form-control" id="inputNP<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.number_phone %>">
                    </div>
                    <div class="form-group col-md-4">
                      <label for="inputEmail">Email</label>
                      <input type="email" class="form-control" id="inputEmail<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.email %>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputEmail">Facebook</label>
                      <input type="email" class="form-control" id="inputPseudoFB<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.facebook %>">
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputEmail">Profession</label>
                      <input type="email" class="form-control" id="inputjob<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.job %>" disabled>
                    </div>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-2">
                      <label for="inputlogement">Nb. adulte</label>
                      <input type="text" class="form-control" id="inputAdult<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.number_adult %>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputlogement">Nb. enfant</label>
                      <input type="text" class="form-control" id="inputChild<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.number_children %> de <%= hostFamilyTab.age_children %>." disabled>
                    </div>
                    <div class="form-group col-md-4">
                      <label for="inputEmail">Animaux</label>
                      <input type="email" class="form-control" id="inputComposition<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.pet_composition %>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputEmail">Stérilisé</label>
                      <input type="email" class="form-control" id="inputSterilized<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.pet_sterilized %>" disabled>
                    </div>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-5">
                      <label for="inputlogement">Logement</label>
                      <input type="text" class="form-control" id="inputLogement<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.type_home %> de <%= hostFamilyTab.area_home %>." disabled>
                    </div>
                      <% if(hostFamilyTab.type_home === 'Appartement avec Jardin'|'Appartement avec Jardin'|'Maison avec cour ou Terrasse'|'Appartement avec cour ou Terrasse'|'Studio avec Jardin'|'Studio avec cour ou Terrasse'|'Terrain/ Caravane/ MobilHome'){ %>
                        <div class="form-group col-md-2">
                          <label for="inputgarden">Exterieur</label>
                          <input type="text" class="form-control" id="inputGarden<%= hostFamilyTab.id %>" value="<%= hostFamilyTab.area_garden %>. " disabled>
                        </div>
                        <div class="form-group col-md-2">
                          <label for="inputgarden">Cloture</label>
                          <input type="text" class="form-control" id="inputfencing<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.fencing === true){%>Oui de <%= hostFamilyTab.fencing_height %>  <% }else{ %>Non<% } %>." disabled>
                        </div>
                      <% } %>
                      <div class="form-group col-md-3">
                        <label for="inputdisponibility">Disponible</label>
                        <% if(hostFamilyTab.disponibility === true) { %>
                            <select type="text" class="form-control" style="background-color: #6efb72c2;" id="inputdisponibility<%= hostFamilyTab.id %>">
                            <option value="true">Oui</option>
                            <option value="false">Non</option>
                          </select>
                          <% }else { %>
                            <select type="text" class="form-control" style="background-color: #f1b510ad;" id="inputdisponibility<%= hostFamilyTab.id %>">
                              <option value="false">Non</option>
                              <option value="true">Oui</option>
                          </select>
                          <% } %> 
                      </div>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-2">
                      <label for="poeple_counsciousness">Dégradation</label>
                      <input type="text" class="form-control" id="poeple_counsciousness<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.poeple_counsciousness%>" disabled>
                    </div>
                    <div class="form-group col-md-2">
                      <label for="poeple_drive_veterinary">Dép. Vétérinaire</label>
                      <input type="text" class="form-control" id="poeple_drive_veterinary<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.poeple_drive_veterinary%>" disabled>
                    </div>
                    <div class="form-group col-md-2">
                      <label for="poeple_car">Dép. Fournitures</label>
                      <input type="text" class="form-control" id="poeple_car<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.poeple_car%>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="family_accept_acceuil">Famille averti</label>
                      <input type="text" class="form-control" id="family_accept_acceuil<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.poeple_car%>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="people_date_accueil">Date de dispo</label>
                      <input type="text" class="form-control" id="people_date_accueil<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.people_date_accueil%>">
                    </div>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-2">
                      <label for="inputCat">Chats acceptés</label>
                      <input type="text" class="form-control" id="inputCat<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.cat_accepted === true){%> Oui <% }else{ %>Non<% } %>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputKitten">Chatons acceptés</label>
                        <input type="text" class="form-control" id="inputKitten<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.kitten_accepted === true){%> Oui <% }else{ %>Non<% } %>" disabled>
                    </div>
                    <div class="form-group col-md-2">
                      <label for="inputPuppy">Chiots acceptés</label>
                        <input type="text" class="form-control" id="inputPuppy<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.puppy_accepted === true){%> Oui <% }else{ %>Non<% } %>" disabled>
                    </div>
                    <div class="form-group col-md-5">
                      <label for="inputpoeple_amity_cat">Connaissance en biberonnage</label>
                        <input type="text" class="form-control" id="inputpoeple_amity_cat<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.poeple_amity_cat === true){%> Oui <% }else{ %>Non<% } %>" disabled>
                    </div>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-3">
                      <label for="inputLittleDog">Chiens (petit) acceptés</label>
                      <input type="text" class="form-control" id="inputLittleDog<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.dog_little_accepted === true){%> Oui <% }else{ %>Non<% } %>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputMiddleDog">Chiens (moyen) acceptés</label>
                        <input type="text" class="form-control" id="inputMiddleDog<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.dog_middle_accepted === true){%> Oui <% }else{ %>Non<% } %>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputBigDog">Chiens (grand) acceptés</label>
                        <input type="text" class="form-control" id="inputBigDog<%= hostFamilyTab.id %>" value="<% if(hostFamilyTab.poeple_dog_big_accepted === true){%> Oui <% }else{ %>Non<% } %>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="inputpoeple_educate">Niveau Education</label>
                        <input type="text" class="form-control" id="inputpoeple_educate<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.inputLittleDog%>" disabled>
                    </div>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-3">
                      <label for="dog_sexe_acceuil">Conditions de sexe</label>
                      <input type="text" class="form-control" id="dog_sexe_acceuil<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.dog_sexe_acceuil%>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="hour_absence_day">Nb. Absence /J</label>
                      <input type="text" class="form-control" id="hour_absence_day<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.hour_absence_day%>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="dog_accept_problem">Animaux à problemes</label>
                      <input type="text" class="form-control" id="dog_accept_problem<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.dog_accept_problem%>" disabled>
                    </div>
                    <div class="form-group col-md-3">
                      <label for="dog_closed_room">Piece d'isolation</label>
                      <input type="text" class="form-control" id="dog_closed_room<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.dog_closed_room %> " disabled>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="poeple_ask">Précisions particulières</label>
                    <textarea type="text" cols="30" rows="1" class="form-control" id="poeple_ask<%= hostFamilyTab.id %>" disabled><%=  hostFamilyTab.poeple_ask%></textarea>
                  </div>
                  <div class="form-group">
                    <label for="poeple_motivation">Motivations</label>
                    <textarea type="text" cols="30" rows="2" class="form-control" id="poeple_motivation<%= hostFamilyTab.id %>" disabled><%= hostFamilyTab.poeple_motivation%></textarea>
                  </div>
                  <hr>
                  <div class="form-row">
                    <div class="form-group col-md-2">
                      <label for="new">Contacté</label>
                      <% if(hostFamilyTab.new === true) { %>
                        <select type="text" class="form-control" id="new<%= hostFamilyTab.id %>">
                          <option value="false">Non</option>
                          <option value="true">Oui</option>
                      </select>
                      <% }else { %>
                        <select type="text" class="form-control" id="new<%= hostFamilyTab.id %>">
                          <option value="true">Oui</option>
                          <option value="false">Non</option>
                      </select>
                      <% } %> 
                    </div>
                    <div class="form-group col-md-2">
                      <label for="nolongercontact">Ancienne FA</label>
                      <% if(hostFamilyTab.nolongercontact === true) { %>
                        <select type="text" class="form-control" id="nolongercontact<%= hostFamilyTab.id %>">
                          <option value="true">Oui</option>
                          <option value="false">Non</option>
                      </select>
                      <% }else { %>
                        <select type="text" class="form-control" id="nolongercontact<%= hostFamilyTab.id %>">
                          <option value="false">Non</option>
                          <option value="true">Oui</option>
                      </select>
                      <% } %> 
                    </div>
                    <div class="form-group col-md-8">
                      <label for="pet_asela">Animaux en FA</label>
                      <input type="text" class="form-control" id="pet_asela<%= hostFamilyTab.id %>" value="<%=hostFamilyTab.pet_asela%>">
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="comment">Commentaire</label>
                    <textarea type="text" cols="30" rows="2" class="form-control" id="comment<%= hostFamilyTab.id %>"> <%= hostFamilyTab.comment%> </textarea>
                  </div>
                </form>
              </div>
              <div class="modal-footer threebtn">
                <span class="d-inline-block" tabindex="0" data-toggle="tooltip" title="Envoyer en liste Rouge">
                  <button type="button" class="btn btn-danger blacklistFA">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bug-fill" viewBox="0 0 16 16">
                      <path d="M4.978.855a.5.5 0 1 0-.956.29l.41 1.352A4.985 4.985 0 0 0 3 6h10a4.985 4.985 0 0 0-1.432-3.503l.41-1.352a.5.5 0 1 0-.956-.29l-.291.956A4.978 4.978 0 0 0 8 1a4.979 4.979 0 0 0-2.731.811l-.29-.956z"/>
                      <path d="M13 6v1H8.5v8.975A5 5 0 0 0 13 11h.5a.5.5 0 0 1 .5.5v.5a.5.5 0 1 0 1 0v-.5a1.5 1.5 0 0 0-1.5-1.5H13V9h1.5a.5.5 0 0 0 0-1H13V7h.5A1.5 1.5 0 0 0 15 5.5V5a.5.5 0 0 0-1 0v.5a.5.5 0 0 1-.5.5H13zm-5.5 9.975V7H3V6h-.5a.5.5 0 0 1-.5-.5V5a.5.5 0 0 0-1 0v.5A1.5 1.5 0 0 0 2.5 7H3v1H1.5a.5.5 0 0 0 0 1H3v1h-.5A1.5 1.5 0 0 0 1 11.5v.5a.5.5 0 1 0 1 0v-.5a.5.5 0 0 1 .5-.5H3a5 5 0 0 0 4.5 4.975z"/>
                    </svg>
                  </button>
                </span>
                <span class="pull-right">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal" aria-label="Close"> Fermer</button>
                  <button type="button" class="btn btn-primary editFA">Valider les modifications</button>
                </span>
            </div>
            </div>
          </div>
        </div>
        <% } %> 
    </tbody>
  </table>
</div>

