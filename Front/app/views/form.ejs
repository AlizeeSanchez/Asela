                                <option value="">----Choisir une réponse----</option>
                                <option value="On le prend avec nous">On le prend avec nous</option>
                                <option value="Agence de gardiennage">Agence de gardiennage</option>
                                <option value="Petsitter">Petsitter</option>
                                <option value="Chenil">Chenil</option>
                                <option value="Voisin">Voisin</option>
                                <option value="Famille">Famille</option>
                                <option value="Autre">Autre</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label for="inputguarding2"> Si autre, veuillez préciser : </label>
                            <input type="holiday2" class="form-control" id="holiday2" >
                        </div>
