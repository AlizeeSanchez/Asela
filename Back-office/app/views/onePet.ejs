                    <div class="card">
                        <div class="card-header">
                            Informations importantes
                        </div>
                        <div class="card-body">
                            <div class="information-pet">
                                <h6>Prise en charge:</h6>
                                <p><%= jason.pet.date_supported %></p>
                            </div>
                            <div class="information-pet">
                                <h6>Identification:</h6>
                                <p><%= jason.pet.ide %></p>
                            </div>
                            <div class="information-pet">
                                <h6>
                                    Stérilisation:</h6>
                                <p><%= jason.pet.sterilised %></p>
                            </div>
                            <div class="information-pet">
                                <h6>
                                    Vaccin:</h6>
                                    <% if(jason.pet.date_vaccine){ %>
                                        <p>Oui</p>
                                    </div>
                                    <div class="information-pet">
                                        <h6>
                                            Dernier vaccin:</h6>
                                        <p><%= jason.pet.date_vaccine.toLocaleString('en-GB').substr(0, 10)%></p>
                                    </div>
                                    <% }else { %>
                                        <p>Non</p>
                                    </div>
                                    <%   } %>     
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header">
                            Famille d'acceuil
