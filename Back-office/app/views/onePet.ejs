<%- include('partials/header') %>

<div class="container">
<main class="main-article">
    <div class="card">
        <div class="card-header">
            <h1 class="Title"><%= jason.pet.name %>
                <%if(jason.pet.adopt === true){%>
                    <span class="Title-new-name-pet"> (<%= jason.pet.last_name_pet %>)</span>
                </h1>
                <%}%> 
        </div> 

        <div class="card-body">

            <div class="row">

                <div class="col-sm-8">

                    <div class="card">
                        <div class="card-header">
                            <p>Fiche de <%= jason.pet.name %></p>
                            <div class="mybuttons">
                                <div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups">
                                    <div class="btn-group mr-2" role="group" aria-label="First group">
                                        <%if(jason.pet.facebook_publish === true){%>
                                            <button style="background-color:#087aea;" type="button" class="btn btn-secondary" data-toggle="tooltip" data-placement="top" title="Est publié sur facebook">
                                                Fb
                                            </button>
                                        <%}else{%>
                                            <button type="button" class="btn btn-secondary" data-toggle="tooltip" data-placement="top" title="N'est pas publié sur facebook">
                                                Fb
                                            </button>
                                        <%}%>
                                        <%if(jason.pet.seconde_chance_publish === true){%>
                                      <button style="background-color: #f68d24;" type="button" class="btn btn-secondary" data-toggle="tooltip" data-placement="top" title="Si publié sur facebook">Sc</button>
                                        <%}else{%>
                                            <button type="button" class="btn btn-secondary" data-toggle="tooltip" data-placement="top" title="N'est pas publié sur Seconde chance">
                                                Sc
                                              </button>
                                        <%}%>
                                    </div>
                                    <button type="button" id="editionPetButton" class="btn btn-outline-primary" data-toggle="modal" data-target="#editModalDog">
